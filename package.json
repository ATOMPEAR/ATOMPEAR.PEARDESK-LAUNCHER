    "dev": "concurrently --kill-others \"npm run sass\" \"npm run start\""
  },
  "dependencies": {
    "electron": "^32.2.0",
    "check-disk-space": "^3.4.0"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "sass": "^1.80.5"
  }
}
