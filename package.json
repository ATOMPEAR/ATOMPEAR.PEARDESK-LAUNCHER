{
  "name": "peardesk-launcher",
  "productName": "PEARDESK",
  "version": "00.00.08",
  "description": "PROGRAM LAUNCHER FOR PEARDESK",
  "main": "resources/source/java/main.js",
  "scripts": {
    "run": "electron .",
    "start": "nodemon --exec electron .",
